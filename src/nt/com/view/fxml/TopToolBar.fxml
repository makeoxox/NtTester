<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>

<fx:root stylesheets="@../css/TopToolBar.css"  fx:controller="nt.com.buiz.TopToolBarController" prefHeight="33.0" prefWidth="592.0" type="ToolBar" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
	<items>
		<Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="0.0" />
		<Button fx:id="newfilebtn" onAction="#newFile" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false">
			<graphic>
				<ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../../../res/newfile.gif" />
					</image>
				</ImageView>
			</graphic>
         <tooltip>
            <Tooltip text="新建" />
         </tooltip>
		</Button>
		<Button fx:id="savefilebtn"  onAction="#savaFile"  contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false">
			<graphic>
				<ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../../../res/save.gif" />
					</image>
				</ImageView>
			</graphic>
         <tooltip>
            <Tooltip text="保存" />
         </tooltip>
		</Button>
		<Button fx:id="importfilebtn" onAction="#importPackage"  contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false">
			<graphic>
				<ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../../../res/import.gif" />
					</image>
				</ImageView>
			</graphic>
         <tooltip>
            <Tooltip text="导入" />
         </tooltip>
		</Button>
		<Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="0.0" />
		<Button fx:id="debugbtn"  onAction="#debug" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false">
			<graphic>
				<ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../../../res/jsdebug.gif" />
					</image>
				</ImageView>
			</graphic>
         <tooltip>
            <Tooltip text="调试脚本" />
         </tooltip>
		</Button>
		<Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="0.0" />
		<Button fx:id="runbtn" onAction="#run" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false">
			<graphic>
				<ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../../../res/run.gif" />
					</image>
				</ImageView>
			</graphic>
         <tooltip>
            <Tooltip text="开始发送" />
         </tooltip>
		</Button>
		<Button fx:id="stopbtn" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false">
			<graphic>
				<ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../../../res/stop.gif" />
					</image>
				</ImageView>
			</graphic>
         <tooltip>
            <Tooltip text="中断" />
         </tooltip>
		</Button>
		<Button fx:id="clearbtn" onAction="#clear" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false">
			<graphic>
				<ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../../../res/erase.gif" />
					</image>
				</ImageView>
			</graphic>
         <tooltip>
            <Tooltip text="清空当前控制台" />
         </tooltip>
		</Button>
		<Button fx:id="multibtn" onAction="#find" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false">
			<graphic>
				<ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../../../res/find.png" />
					</image>
				</ImageView>
			</graphic>
         <tooltip>
            <Tooltip text="文本查找" />
         </tooltip>
		</Button>
		<Separator  orientation="VERTICAL" prefHeight="0.0" prefWidth="0.0" />
		<Button fx:id="setbtn" onAction="#msgset" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false">
			<graphic>
				<ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../../../res/msgset.gif" />
					</image>
				</ImageView>
			</graphic>
         <tooltip>
            <Tooltip text="报文设置" />
         </tooltip>
		</Button>
		<Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="0.0" />
		<Button fx:id="multibtn" onAction="#multiset" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" mnemonicParsing="false">
			<graphic>
				<ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
					<image>
						<Image url="@../../../../res/multiset.png" />
					</image>
				</ImageView>
			</graphic>
         <tooltip>
            <Tooltip text="并发配置" />
         </tooltip>
		</Button>
		<Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="0.0" />
	</items>
</fx:root>
